    <tr class='<%= tee_box.color.downcase %>_tee_box'>
      <td><%= tee_box.color %></td>
      <td><%= tee_box.tee_box_type.tee_box_type %></td>
      <% if tee_box.tee_box_type.tee_box_type == TeeBoxType.mens %>
        <td><%= tee_box.mens_course_rating %>/<%= tee_box.mens_course_slope %></td>
        <td></td>
      <% else if tee_box.tee_box_type.tee_box_type == TeeBoxType.ladies %>
        <td></td>
        <td><%= tee_box.ladies_course_rating %>/<%= tee_box.ladies_course_slope %></td>
      <% else %>
        <td><%= tee_box.mens_course_rating %>/<%= tee_box.mens_course_slope %></td>
        <td><%= tee_box.ladies_course_rating %>/<%= tee_box.ladies_course_slope %></td>
      <% end end %>
      <% tee_box.holes.each do |hole| %>
        <td><%=hole.hole_number %></td>
      <% end %>
      <td></td>
    </tr>
